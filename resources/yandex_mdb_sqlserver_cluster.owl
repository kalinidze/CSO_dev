    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_mdb_sqlserver_cluster_deletion_protection">
    <rdfs:label>deletion_protection</rdfs:label>
    <rdfs:comment>The `true` value means that resource is protected from accidental deletion.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_mdb_sqlserver_cluster"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
