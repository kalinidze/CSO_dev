  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_organizationmanager_group_mapping_enabled">
    <rdfs:label>enabled</rdfs:label>
    <rdfs:comment>Set "true" to enable organization manager group mapping.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_organizationmanager_group_mapping"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_organizationmanager_group_mapping_federation_id">
    <rdfs:label>federation_id</rdfs:label>
    <rdfs:comment>ID of the SAML Federation.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_organizationmanager_group_mapping"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_organizationmanager_group_mapping_id">
