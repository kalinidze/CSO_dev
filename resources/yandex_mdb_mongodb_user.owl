  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="http://example.org/onto/">
  <owl:Class rdf:about="http://example.org/onto/class/yandex_mdb_mongodb_user">
