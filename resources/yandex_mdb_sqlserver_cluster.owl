    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_mdb_sqlserver_cluster"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_mdb_sqlserver_cluster_resource_preset_id">
    <rdfs:label>resource_preset_id</rdfs:label>
    <rdfs:comment>The ID of the preset for computational resources available to a SQLServer host (CPU, memory etc.). For more information, see [the official documentation](https://yandex.cloud/docs/managed-sqlserver/concepts/instance-types).</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_mdb_sqlserver_cluster"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
