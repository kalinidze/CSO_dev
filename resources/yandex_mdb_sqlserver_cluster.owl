  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_mdb_sqlserver_cluster_sqlserver_config">
    <rdfs:label>sqlserver_config</rdfs:label>
    <rdfs:comment>SQLServer cluster config. Detail info in `SQLServer config` section.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_mdb_sqlserver_cluster"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
