    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_organizationmanager_saml_federation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_organizationmanager_saml_federation_delete">
    <rdfs:label>delete</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_organizationmanager_saml_federation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_organizationmanager_saml_federation_read">
    <rdfs:label>read</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_organizationmanager_saml_federation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_organizationmanager_saml_federation_update">
