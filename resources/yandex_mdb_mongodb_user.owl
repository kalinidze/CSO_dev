  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_mdb_mongodb_user_password">
    <rdfs:label>password</rdfs:label>
    <rdfs:comment>The password of the user.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_mdb_mongodb_user"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_mdb_mongodb_user_timeouts">
    <rdfs:label>timeouts</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_mdb_mongodb_user"/>
