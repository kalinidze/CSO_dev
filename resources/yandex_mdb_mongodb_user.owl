    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_mdb_mongodb_user"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_mdb_mongodb_user_roles">
    <rdfs:label>roles</rdfs:label>
