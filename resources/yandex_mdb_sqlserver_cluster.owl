  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_mdb_sqlserver_cluster_environment">
    <rdfs:label>environment</rdfs:label>
    <rdfs:comment>Deployment environment of the SQLServer cluster. (PRODUCTION, PRESTABLE).</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_mdb_sqlserver_cluster"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
