<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="http://example.org/onto/">
  <owl:Class rdf:about="http://example.org/onto/class/yandex_compute_image">
    <rdfs:label>yandex_compute_image</rdfs:label>
    <rdfs:comment>Creates a virtual machine image resource for the Yandex Compute Cloud service from an existing tarball. For more information, see [the official documentation](https://yandex.cloud/docs/compute/concepts/image).

~> One of `source_family`, `source_image`, `source_snapshot`, `source_disk` or `source_url` must be specified.</rdfs:comment>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_created_at">
    <rdfs:label>created_at</rdfs:label>
    <rdfs:comment>The creation timestamp of the resource.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_description">
    <rdfs:label>description</rdfs:label>
    <rdfs:comment>The resource description.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_family">
    <rdfs:label>family</rdfs:label>
    <rdfs:comment>The name of the image family to which this image belongs.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_folder_id">
    <rdfs:label>folder_id</rdfs:label>
    <rdfs:comment>The folder identifier that resource belongs to. If it is not provided, the default provider `folder-id` is used.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_id">
    <rdfs:label>id</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_labels">
    <rdfs:label>labels</rdfs:label>
    <rdfs:comment>A set of key/value label pairs which assigned to resource.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_min_disk_size">
    <rdfs:label>min_disk_size</rdfs:label>
    <rdfs:comment>Minimum size in GB of the disk that will be created from this image.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_name">
    <rdfs:label>name</rdfs:label>
    <rdfs:comment>The resource name.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_os_type">
    <rdfs:label>os_type</rdfs:label>
    <rdfs:comment>Operating system type that is contained in the image. Possible values: `LINUX`, `WINDOWS`.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_pooled">
    <rdfs:label>pooled</rdfs:label>
    <rdfs:comment>Optimize the image to create a disk.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_product_ids">
    <rdfs:label>product_ids</rdfs:label>
    <rdfs:comment>License IDs that indicate which licenses are attached to this image.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_size">
    <rdfs:label>size</rdfs:label>
    <rdfs:comment>The size of the image, specified in GB.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_source_disk">
    <rdfs:label>source_disk</rdfs:label>
    <rdfs:comment>The ID of a disk to use as the source of the image. Changing this ID forces a new resource to be created.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_source_family">
    <rdfs:label>source_family</rdfs:label>
    <rdfs:comment>The name of the family to use as the source of the new image. The ID of the latest image is taken from the `standard-images` folder. Changing the family forces a new resource to be created.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_source_image">
    <rdfs:label>source_image</rdfs:label>
    <rdfs:comment>The ID of an existing image to use as the source of the image. Changing this ID forces a new resource to be created.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_source_snapshot">
    <rdfs:label>source_snapshot</rdfs:label>
    <rdfs:comment>The ID of a snapshot to use as the source of the image. Changing this ID forces a new resource to be created.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_source_url">
    <rdfs:label>source_url</rdfs:label>
    <rdfs:comment>The URL to use as the source of the image. Changing this URL forces a new resource to be created.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_status">
    <rdfs:label>status</rdfs:label>
    <rdfs:comment>The status of the image.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_pci_topology">
    <rdfs:label>pci_topology</rdfs:label>
    <rdfs:comment>A variant of PCI topology, one of `PCI_TOPOLOGY_V1` or `PCI_TOPOLOGY_V2`.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_create">
    <rdfs:label>create</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_delete">
    <rdfs:label>delete</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/onto/prop/yandex_compute_image_update">
    <rdfs:label>update</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_compute_image"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
</rdf:RDF>
