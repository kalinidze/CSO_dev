    <rdfs:label>connection_string</rdfs:label>
    <rdfs:comment>Connection string, conflicts with `table_id`.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/onto/class/yandex_ydb_table_changefeed"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
